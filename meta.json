{
    "prompts": {
        "name": {
            "type": "string",
            "required": true,
            "label": "项目名称"
        },
        "description": {
            "type": "string",
            "required": true,
            "label": "项目描述",
            "default": "A Vue.js project"
        },
        "author": {
            "type": "string",
            "label": "主要作者"
        },
        "vuex": {
            "type": "confirm",
            "message": "是否安装 vuex?"
        }
    },
    "filters": {
        "src/store/**/*": "vuex"
    },
    "completeMessage": "项目创建完毕，请继续执行如下命令:\n\n npm install\n npm run build:lib\n npm run dev"
}